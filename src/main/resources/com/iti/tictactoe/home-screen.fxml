<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="1080" prefWidth="1920.0" styleClass="background-style"
            stylesheets="@styles.css" xmlns="http://javafx.com/javafx/8.0.171"
            fx:controller="com.iti.tictactoe.HomeScreenController">
   <children>

      <Button fx:id="startPlayingButton" alignment="CENTER" layoutX="729.0" layoutY="433.0" onAction="#onPlayButton"
              prefHeight="89.0" prefWidth="463.0" styleClass="startbtn" stylesheets="@styles.css" text="Start playing"
              textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font name="Comic Sans MS Bold" size="38.0" />
         </font>
      </Button>
      <Button fx:id="quitButton" alignment="CENTER" layoutX="729.0" layoutY="558.0" onAction="#onQuitButton"
              prefHeight="89.0" prefWidth="463.0" styleClass="quitbtn" stylesheets="@styles.css" text="Quit"
              textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font name="Comic Sans MS Bold" size="38.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
