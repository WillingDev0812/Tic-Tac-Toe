<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="1080.0" prefWidth="1920.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.iti.tictactoe.Recordings">
    <children>
        <ImageView fx:id="main_background_img" fitHeight="1080" fitWidth="1920.0" layoutX="-14.0" layoutY="-14.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@assest/test.png" />

            </image>
        </ImageView>
      <Text fill="WHITE" layoutX="793.0" layoutY="170.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Recordings" wrappingWidth="305.6708984375">
         <font>
            <Font name="Comic Sans MS Bold" size="52.0" />
         </font>
      </Text>
      <ImageView fitHeight="99.0" fitWidth="93.0" layoutX="207.0" layoutY="102.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image fx:id="backimg" url="@assest/back-button%20(1).png" />  <!--  here -->
         </image>
      </ImageView>
      <Text fill="WHITE" layoutX="300.0" layoutY="162.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Back" wrappingWidth="84.6708984375">
         <font>
            <Font name="Comic Sans MS Bold" size="25.0" />
         </font>
      </Text>
      <ScrollPane fx:id="scrollablepane" layoutX="492.0" layoutY="242.0" opacity="0.2" prefHeight="754.0" prefWidth="909.0">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="200.0" prefWidth="200.0" />
        </content>
      </ScrollPane>
        <Button fx:id="pa" alignment="CENTER" layoutX="207.0" layoutY="907.0" onAction="#onplaybtn" prefHeight="89.0" prefWidth="222.0" styleClass="startbtn" stylesheets="@styles.css" text="Play Recording" textAlignment="CENTER" textFill="WHITE">
            <font>
                <Font name="Comic Sans MS Bold" size="25.0" />
            </font>
        </Button>
        <Button fx:id="quitbtn" alignment="CENTER" layoutX="1422.0" layoutY="907.0" onAction="#onquitbtn" prefHeight="89.0" prefWidth="222.0" styleClass="quitbtn" stylesheets="@styles.css" text="Delete Recording" textAlignment="CENTER" textFill="WHITE">
            <font>
                <Font name="Comic Sans MS Bold" size="30.0" />
            </font>
        </Button>

    </children>
</AnchorPane>
